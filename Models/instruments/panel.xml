<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ALTIMETER -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  -0.315 </x-m>
      <y-m>  0.294 </y-m>
      <z-m>  0.273 </z-m>
    </offsets>
  </model>
  
  <!-- TURN -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/turn/turn.xml</path>
    <offsets>
      <x-m> -0.315 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.272 </z-m>
    </offsets>
  </model>
  
  <!-- FUEL SELECTOR -->
  <model>
    <path>Aircraft/Aviatik-CI/Models/instruments/fuelselector/fuelselector.xml</path>
    <offsets>
      <x-m> -0.314 </x-m>
      <y-m> -0.407 </y-m>
      <z-m> 0.263 </z-m>
    </offsets>
  </model>
  
